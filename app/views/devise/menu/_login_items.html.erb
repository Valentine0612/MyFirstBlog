<% if user_signed_in? %>
  <li class="nav-item">
    <a class="nav-link" href="<%= feed_path %>"><%= t :Feed %></a>
  </li>
  <li class="nav-item">
    <a class="nav-link" href="<%= saves_path %>"><%= t :Saves %></a>
  </li>
  <li class="nav-item">
    <%= link_to t(:Logout), destroy_user_session_path, method: :delete, class: "nav-link" %>
  </li>
<% else %>
  <li class="nav-item">
    <%= link_to t(:Login), new_user_session_path, class: "nav-link"  %>
  </li>
<% end %>